        "sinon": "1.17.3",
        "sinon-chai": "2.8.0",
        "webpack": "1.12.14",
        "webpack-dev-middleware": "1.5.1",
        "webpack-hot-middleware": "2.10.0"
    },
    "dependencies": {
        "babel-polyfill": "6.7.2",
        "ramda": "0.19.1",
        "react": "0.14.7",
