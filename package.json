{
    "name": "react-clientside-example",
    "version": "0.0.0",
    "description": "Example for client side rendered React and Redux",
    "main": "index.js",
    "private": true,
    "scripts": {
        "lint": "eslint .",
        "build": "NODE_ENV=production browserify app.js -t babelify | uglifyjs -c > build/bundle.js",
        "build:dev": "watchify app.js -t babelify -o build/bundle.js -v -d",
        "dev": "npm run build:dev & node-dev server.js",
        "start": "npm run build && node server.js",
        "pretest": "npm run lint",
        "test": "npm run test:unit:cover",
        "test:unit": "mocha test/unit --opts test/unit/mocha.opts",
        "test:unit:cover": "istanbul cover _mocha test/unit -- --opts test/unit/mocha.opts"
    },
    "author": "Alexander Schmidt <alexanderschmidt1@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "babel-polyfill": "6.8.0",
        "babel-preset-es2015": "6.6.0",
        "babel-preset-react": "6.5.0",
        "babel-register": "6.8.0",
        "babelify": "7.3.0",
        "bootstrap": "3.3.6",
        "browserify": "13.0.1",
        "chai": "3.5.0",
        "enzyme": "2.3.0",
        "eslint": "2.10.0",
        "eslint-config-holidaycheck": "0.7.0",
        "eslint-plugin-mocha": "2.2.0",
        "eslint-plugin-react": "5.1.1",
        "expect": "1.20.1",
        "express": "4.13.4",
        "istanbul": "1.0.0-alpha.2",
        "jsdom": "9.0.0",
        "mocha": "2.4.5",
        "node-dev": "3.1.1",
        "react-addons-test-utils": "15.0.2",
        "sinon": "1.17.4",
        "sinon-chai": "2.8.0",
        "uglifyjs": "2.4.10",
        "watchify": "3.7.0"
    },
    "dependencies": {
        "ramda": "0.21.0",
        "react": "15.0.2",
        "react-bootstrap": "0.29.4",
        "react-dom": "15.0.2",
        "react-redux": "4.4.5",
        "redux": "3.5.2"
    }
}
