{
    "name": "react-clientside-example",
    "version": "0.0.0",
    "description": "Example for client side rendered React with hot reloading",
    "main": "index.js",
    "private": true,
    "scripts": {
        "lint": "eslint .",
        "start": "babel-node server.js",
        "pretest": "npm run lint",
        "test": "npm run test:unit:cover",
